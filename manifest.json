{
  "name": "TradeLocker Plugin",
  "description": "A simple Chrome extension for demo.tradelocker.com",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "https://demo.tradelocker.com/es/trade"
      ],
      "js": ["content.js"]
    }
  ]
}