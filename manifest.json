{
  "name": "TradeLocker Plugin",
  "description": "A simple Chrome extension for demo.tradelocker.com",
  "version": "1.0",
  "background": {
    "service_worker": "service-worker.js",
    "type": "module"
  },
  "manifest_version": 3,
  "action": {
    "default_popup": "popup.html"
  },
  "permissions":[
    "scripting"
  ],
  "host_permissions": [
    "https://demo.tradelocker.com/es/trade",
    "https://simul8or.com/"
  ]
}